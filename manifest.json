{
    "manifest_version":2,
    "name":"Unlock Content",
    "version":"0.0.1",
    "description":"Unlock Content",

    "content_scripts": [
        {
            "matches": [
                "*://*.folha.uol.com.br/*",
                "*://*.oglobo.globo.com/*",
                "*://*.valor.globo.com/*",
                "*://*.veja.abril.com.br/*"
            ],
            "js": ["content.js"]
        }
    ],

    "browser_action": {
        "default_icon": "document.png",
        "default_title": "Test",
        "default_popup": "index.html"
    }

}